{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node",
			"config": {
				"includeFiles": ["dist/**", "node_modules/**", "prisma/schema.prisma"],
				"env": {
					"TZ": "America/Sao_Paulo"
				}
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts"
		}
	]
}
